<Defaults>
    <Text class="teamLabel" fontSize="48" alignment="UpperCenter" offsetXY = "0 -80"/>

    <VerticalLayout class = "gameButtonsLayout"
        height = "150"
        width = "180"
        rectAlignment = "LowerRight"
        offsetXY = "-100 7"
        visibility = ""
        active = "false"
        />

    <TableLayout class = "playingButtonsLayout"
        height = "50"
        width = "600"
        rectAlignment = "LowerCenter"
        offsetXY = "0 7"
        visibility = ""
        active = "false"
        />

    <VerticalLayout class = "singleButtonLayout"
        height = "50"
        width = "200"
        rectAlignment = "LowerCenter"
        offsetXY = "0 7"
        active = "false"
        />

    <Cell class = "buttonsCell" columnSpan = "1" dontUseTableCellBackground = "true"/>
    <Cell class = "emptyCell" width = "200" dontUseTableCellBackground = "true"/>
    <Button class = "actionButton" width = "200" />
    <Button class = "singleButton" height = "50" width = "200" rectAlignment="LowerCenter" />
    <Button class = "redButton" colors="#FF404060|#FF4040FF|#FF404080|#FF404080" textColor="#FFFFFF"/>
    <Button class = "blueButton" colors="#4040FF60|#4040FFFF|#4040FF80|#4040FF80" textColor="#FFFFFF"/>
    <Button class = "blackButton" colors="#66666680|#666666FF|#66666680|#66666680" textColor="#FFFFFF"/>
</Defaults>
